{
    "name":"vasildakov/site-package",
    "type":"typo3-cms-extension",
    "description":"Example site package from the site package tutorial",
    "homepage":"https://github.com/vasildakov/typo3-custom-extension/",
    "license":"MIT",
    "keywords":["typo3", "site package"],
    "version": "1.0.1",
    "support":{
        "issues":"https://github.com/vasildakov/typo3-custom-extension/issues"
    },
    "authors":[
        {
            "name":"Vasil Dakov",
            "role":"Developer",
            "homepage":"https://github.com/vasildakov/"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0 || ^8.1 || ^8.2",
        "typo3/cms-core":"^11.5",
        "typo3/cms-fluid-styled-content":"^11.5"
    },
    "require-dev": {
        "typo3/testing-framework": "^7.0",
        "squizlabs/php_codesniffer": "^3.9",
        "phpstan/phpstan": "^1.11",
        "phpunit/phpunit": "^10.5",
        "phpro/grumphp": "^2.5"
    },
    "autoload":{
        "psr-4":{
            "VasilDakov\\SitePackage\\":"Classes/"
        }
    },
    "autoload-dev":{
        "psr-4":{
            "VasilDakov\\SitePackage\\Tests\\":"Tests/"
        }
    },
    "extra":{
        "typo3/cms":{
            "extension-key":"site_package"
        }
    },
    "config": {
        "allow-plugins": true,
        "vendor-dir": ".composer/vendor",
        "bin-dir": ".composer/bin"
    }
}
